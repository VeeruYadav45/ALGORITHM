class Solution {
public:
    void sortZeroOneTwo(vector<int>& nums) {
        int low=0; int mid=0;
        int high=nums.size()-1;
        while(mid<=high){
            if(a[mid]==0){
                swap(nums[mid],nums[low]);
                mid++; low++;
            }
           else if(a[mid]==1){
                mid++;
            }
            else {
                swap(nums[mid],nums[high]); high--;
            }
        }
    }
};
