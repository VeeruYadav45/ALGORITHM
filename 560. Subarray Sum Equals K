class Solution {
public:
    int subarraySum(vector<int>& nums, int k) {
        int p=0;
        int c=0;
        map<int,int>v;
        v[0]=1; int r=0;
        for(int i=0;i<nums.size();i++){
            p+=nums[i];//prefix sum
            r=p-k;//elment to be removed 
            c+=v[r];//count
            v[p]+=1;//addding count in map
        }
        return c;
    }
};
